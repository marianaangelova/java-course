import java.util.Scanner;
public class FifthTask {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in,"UTF-8");
		int a = input.nextInt();
		int b = input.nextInt();
		int c = input.nextInt();
		int d = input.nextInt();
		input.close();
		System.out.println("Sum: "+(a + b + c + d));
	}

}
