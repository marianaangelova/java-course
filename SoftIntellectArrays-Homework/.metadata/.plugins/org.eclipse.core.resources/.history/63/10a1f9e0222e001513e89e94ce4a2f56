
public class NumbersTo30 {

	public static void main(String[] args) {
		int[] array = new int[30];
		
		for(int i = array.length;i >=1;i--) {
			array[i] = i;
			if(array[i] == 25 || array[i] == 20 || array[i] == 15 || array[i] == 10 || array[i] == 5){
				System.out.println();
			}
			System.out.print(array[i] + " ");
		}
	}

}