public class NumbersFrom1To30000 {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		int n = input.nextInt();
		int[] array = new int[n];
		for (int i = 0; i < n; i++) {
			array[i] = input.nextInt();
		}
		reverseArray(array);
		System.out.println(Arrays.toString(array));
	}
