
public class SymetricNumbers {

	public static void main(String[] args) {
		
	}
	public static void PrintSymetricNumbers(int a ,int b) {
		a = 11; b = 999;
		
		for (int i = a; i <= b; i++) {
			if (i >= a && i < 100) {
				if ((i % 10) == (i / 10)) {
					System.out.println(i+" ");
				}
			}
			else if (i >= 100) {
				if ((i % 10) == (i / 100)) {
					System.out.println(i+" ");
				}
			}
		}
	}

}
