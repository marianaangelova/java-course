
public class ClassExercise_3 {

	public static void main(String[] args) {
		int counter = 1;
		System.out.println("With \"for\" loop:");
		for(int i = 1;i <= 100;i++) {
		if((i % 7 == 0) || (i % 11 == 0)) {
				System.out.print(i+ " ");
			}
		}
		System.out.println();
		System.out.println();
		System.out.println("With \"while\" loop:");
		while(counter <= 100) {
			counter++;
			if((counter % 7 == 0) || (counter % 11 == 0)) {
				System.out.print(counter+" ");
				
			}
		}
		System.out.println();
		System.out.println();
		System.out.println("With \"do-while\" loop:");
		 do {
			counter++;
			if((counter % 7 == 0) || (counter % 11 == 0)) {
				System.out.print(counter+" ");
				
			}
		}while(counter <= 100);
	}

}
